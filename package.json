    "chalk": "^0.5.1",
    "chokidar": "0.12.6",
    "commander": "2.6.0",
    "core-js": "^0.4.9",
    "debug": "^2.1.1",
    "detect-indent": "3.0.0",
    "esgraph": "^0.1.1",
    "estraverse": "1.9.1",
    "esutils": "1.1.6",
    "fs-readdir-recursive": "0.1.0",
    "globals": "^5.1.0",
    "js-tokenizer": "1.3.3",
    "lodash": "3.0.0",
    "output-file-sync": "1.1.0",
    "private": "0.1.6",
